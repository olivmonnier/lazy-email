{
  "name": "lazy-email",
  "version": "1.0.0",
  "description": "A Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --all",
    "build-mac": "electron-packager . --platform=mas --out=dist/ --overwrite",
    "build-win": "electron-packager . --platform=win32 --out=dist/ --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olivmonnier/lazy-email.git"
  },
  "author": "olivmonnier",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/olivmonnier/lazy-email/issues"
  },
  "homepage": "https://github.com/olivmonnier/lazy-email#readme",
  "devDependencies": {
    "electron": "^1.3.4",
    "electron-packager": "^8.0.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "cheerio": "^0.22.0",
    "codemirror": "^5.19.0",
    "electron-config": "^0.2.1",
    "font-awesome": "^4.6.3",
    "inky": "^1.3.7",
    "inline-css": "^2.2.2",
    "jquery": "^3.1.1",
    "nunjucks": "^2.5.2"
  }
}
